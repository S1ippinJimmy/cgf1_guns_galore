<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<ThingCategoryDef>
		<defName>Ammo762x17mmRimless</defName>
		<label>7.62x17mm Rimless</label>
		<parent>AmmoPistols</parent>
		<iconPath>UI/Icons/ThingCategories/CaliberPistol</iconPath>
	</ThingCategoryDef>

	<!-- ==================== AmmoSet ========================== -->

	<CombatExtended.AmmoSetDef MayRequire="cgf1.gunsgalore.coldwar.chinavietnam">
		<defName>AmmoSet_762x17mmRimless</defName>
		<label>7.62x17mm Rimless</label>
		<ammoTypes>
			<Ammo_762x17mmRimless_FMJ>Bullet_762x17mmRimless_FMJ</Ammo_762x17mmRimless_FMJ>
			<Ammo_762x17mmRimless_AP>Bullet_762x17mmRimless_AP</Ammo_762x17mmRimless_AP>
