		<description>Craft 500 7.62x17mm Rimless (FMJ) cartridges.</description>
		<jobString>Making 7.62x17mm Rimless (FMJ) cartridges.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Steel</li>
					</thingDefs>
				</filter>
				<count>8</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Steel</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Ammo_762x17mmRimless_FMJ>500</Ammo_762x17mmRimless_FMJ>
