<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingCategoryDef>
		<defName>Ammo10mmMagnum</defName>
		<label>10mm Magnum</label>
		<parent>AmmoPistols</parent>
		<iconPath>UI/Icons/ThingCategories/CaliberPistol</iconPath>
	</ThingCategoryDef>

	<!-- ==================== AmmoSet ========================== -->

	<CombatExtended.AmmoSetDef MayRequire="cgf1.gunsgalore.magnum.i">
		<defName>AmmoSet_10mmMagnum</defName>
		<label>10mm Magnum</label>
		<ammoTypes>
			<Ammo_10mmMagnum_FMJ>Bullet_10mmMagnum_FMJ</Ammo_10mmMagnum_FMJ>
			<Ammo_10mmMagnum_AP>Bullet_10mmMagnum_AP</Ammo_10mmMagnum_AP>
			<Ammo_10mmMagnum_HP>Bullet_10mmMagnum_HP</Ammo_10mmMagnum_HP>			
		</ammoTypes>
		<similarTo>AmmoSet_Pistol</similarTo>
	</CombatExtended.AmmoSetDef>

	<!-- ==================== Ammo ========================== -->

	<ThingDef Class="CombatExtended.AmmoDef" Name="10mmMagnumBase" ParentName="SmallAmmoBase" Abstract="True" MayRequire="cgf1.gunsgalore.magnum.i">
		<description>A lengthened and modified 10mm Auto cartridge with increased power and speed</description>
		<statBases>
			<Mass>0.025</Mass>
			<Bulk>0.01</Bulk>
		</statBases>
		<tradeTags>
			<li>CE_AutoEnableTrade</li>
			<li>CE_AutoEnableCrafting</li>
		</tradeTags>
		<thingCategories>
			<li>Ammo10mmMagnum</li>
		</thingCategories>
	</ThingDef>

	<ThingDef Class="CombatExtended.AmmoDef" ParentName="10mmMagnumBase" MayRequire="cgf1.gunsgalore.magnum.i">
		<defName>Ammo_10mmMagnum_FMJ</defName>
		<label>10mm Magnum cartridge (FMJ)</label>
		<graphicData>
			<texPath>Things/Ammo/Pistol/FMJ</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>0.11</MarketValue>
		</statBases>
		<ammoClass>FullMetalJacket</ammoClass>
		<cookOffProjectile>Bullet_10mmMagnum_FMJ</cookOffProjectile>
	</ThingDef>

	<ThingDef Class="CombatExtended.AmmoDef" ParentName="10mmMagnumBase" MayRequire="cgf1.gunsgalore.magnum.i">
		<defName>Ammo_10mmMagnum_AP</defName>
		<label>10mm Magnum cartridge (AP)</label>
		<graphicData>
			<texPath>Things/Ammo/Pistol/AP</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>0.11</MarketValue>
		</statBases>
		<ammoClass>ArmorPiercing</ammoClass>
		<cookOffProjectile>Bullet_10mmMagnum_AP</cookOffProjectile>
	</ThingDef>

	<ThingDef Class="CombatExtended.AmmoDef" ParentName="10mmMagnumBase" MayRequire="cgf1.gunsgalore.magnum.i">
		<defName>Ammo_10mmMagnum_HP</defName>
		<label>10mm Magnum cartridge (HP)</label>
		<graphicData>
			<texPath>Things/Ammo/Pistol/HP</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>0.11</MarketValue>
		</statBases>
		<ammoClass>HollowPoint</ammoClass>
		<cookOffProjectile>Bullet_10mmMagnum_HP</cookOffProjectile>
	</ThingDef>

	<!-- ================== Projectiles ================== -->

	<ThingDef Name="Base10mmMagnumBullet" ParentName="BaseBulletCE" Abstract="true" MayRequire="cgf1.gunsgalore.magnum.i">
		<graphicData>
			<texPath>Things/Projectile/Bullet_Small</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<projectile Class="CombatExtended.ProjectilePropertiesCE">
			<damageDef>Bullet</damageDef>
			<speed>99</speed>
			<dropsCasings>true</dropsCasings>
			<casingMoteDefname>Fleck_PistolAmmoCasings</casingMoteDefname>
			<casingFilthDefname>Filth_PistolAmmoCasings</casingFilthDefname>
		</projectile>
	</ThingDef>

	<ThingDef ParentName="Base10mmMagnumBullet" MayRequire="cgf1.gunsgalore.magnum.i">
		<defName>Bullet_10mmMagnum_FMJ</defName>
		<label>10mm Magnum bullet (FMJ)</label>
		<projectile Class="CombatExtended.ProjectilePropertiesCE">
			<damageAmountBase>16</damageAmountBase>
			<armorPenetrationSharp>5</armorPenetrationSharp>
			<armorPenetrationBlunt>31.48</armorPenetrationBlunt>
		</projectile>
	</ThingDef>

	<ThingDef ParentName="Base10mmMagnumBullet" MayRequire="cgf1.gunsgalore.magnum.i">
		<defName>Bullet_10mmMagnum_AP</defName>
		<label>10mm Magnum bullet (AP)</label>
		<projectile Class="CombatExtended.ProjectilePropertiesCE">
			<damageAmountBase>10</damageAmountBase>
			<armorPenetrationSharp>10</armorPenetrationSharp>
			<armorPenetrationBlunt>31.48</armorPenetrationBlunt>
		</projectile>
	</ThingDef>

	<ThingDef ParentName="Base10mmMagnumBullet" MayRequire="cgf1.gunsgalore.magnum.i">
		<defName>Bullet_10mmMagnum_HP</defName>
		<label>10mm Magnum bullet (HP)</label>
		<projectile Class="CombatExtended.ProjectilePropertiesCE">
			<damageAmountBase>21</damageAmountBase>
			<armorPenetrationSharp>3</armorPenetrationSharp>
			<armorPenetrationBlunt>31.48</armorPenetrationBlunt>
		</projectile>
	</ThingDef>

	<!-- ==================== Recipes ========================== -->

	<RecipeDef ParentName="AmmoRecipeBase" MayRequire="cgf1.gunsgalore.magnum.i">
		<defName>MakeAmmo_10mmMagnum_FMJ</defName>
		<label>make 10mm Magnum cartridge x500</label>
		<description>Craft 500 10mm Magnum cartridges.</description>
		<jobString>Making 10mm Magnum cartridges.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Steel</li>
					</thingDefs>
				</filter>
				<count>26</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Steel</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Ammo_10mmMagnum_FMJ>500</Ammo_10mmMagnum_FMJ>
		</products>
		<workAmount>2600</workAmount>
	</RecipeDef>

	<RecipeDef ParentName="AmmoRecipeBase" MayRequire="cgf1.gunsgalore.magnum.i">
		<defName>MakeAmmo_10mmMagnum_AP</defName>
		<label>make 10mm Magnum (AP) cartridge x500</label>
		<description>Craft 500 10mm Magnum (AP) cartridges.</description>
		<jobString>Making 10mm Magnum (AP) cartridges.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Steel</li>
					</thingDefs>
				</filter>
				<count>26</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Steel</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Ammo_10mmMagnum_AP>500</Ammo_10mmMagnum_AP>
		</products>
		<workAmount>3120</workAmount>
	</RecipeDef>

	<RecipeDef ParentName="AmmoRecipeBase" MayRequire="cgf1.gunsgalore.magnum.i">
		<defName>MakeAmmo_10mmMagnum_HP</defName>
		<label>make 10mm Magnum (HP) cartridge x500</label>
		<description>Craft 500 10mm Magnum (HP) cartridges.</description>
		<jobString>Making 10mm Magnum (HP) cartridges.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Steel</li>
					</thingDefs>
				</filter>
				<count>26</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Steel</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Ammo_10mmMagnum_HP>500</Ammo_10mmMagnum_HP>
		</products>
		<workAmount>2600</workAmount>
	</RecipeDef>

</Defs>