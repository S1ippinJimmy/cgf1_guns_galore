<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingCategoryDef>
		<defName>Ammo8mmGasser</defName>
		<label>8mm Gasser</label>
		<parent>AmmoPistols</parent>
		<iconPath>UI/Icons/ThingCategories/CaliberPistol</iconPath>
	</ThingCategoryDef>

	<!-- ==================== AmmoSet ========================== -->

	<CombatExtended.AmmoSetDef MayRequire="cgf1.gunsgalore.greatwar.centralpowers">
		<defName>AmmoSet_8mmGasser</defName>
		<label>8mm Gasser</label>
		<ammoTypes>
			<Ammo_8mmGasser_FMJ>Bullet_8mmGasser_FMJ</Ammo_8mmGasser_FMJ>
			<Ammo_8mmGasser_AP>Bullet_8mmGasser_AP</Ammo_8mmGasser_AP>
			<Ammo_8mmGasser_HP>Bullet_8mmGasser_HP</Ammo_8mmGasser_HP>			
		</ammoTypes>
		<similarTo>AmmoSet_Pistol</similarTo>
	</CombatExtended.AmmoSetDef>

	<!-- ==================== Ammo ========================== -->

	<ThingDef Class="CombatExtended.AmmoDef" Name="8mmGasserBase" ParentName="SmallAmmoBase" Abstract="True" MayRequire="cgf1.gunsgalore.greatwar.centralpowers">
		<description>Old cartridge designed for revolvers and some semi-auto pistols.</description>
		<statBases>
			<Mass>0.014</Mass>
			<Bulk>0.01</Bulk>
		</statBases>
		<tradeTags>
			<li>CE_AutoEnableTrade</li>
			<li>CE_AutoEnableCrafting</li>
		</tradeTags>
		<thingCategories>
			<li>Ammo8mmGasser</li>
		</thingCategories>
	</ThingDef>

	<ThingDef Class="CombatExtended.AmmoDef" ParentName="8mmGasserBase" MayRequire="cgf1.gunsgalore.greatwar.centralpowers">
		<defName>Ammo_8mmGasser_FMJ</defName>
		<label>8mm Gasser cartridge (FMJ)</label>
		<graphicData>
			<texPath>Things/Ammo/Pistol/FMJ</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>0.06</MarketValue>
		</statBases>
		<ammoClass>FullMetalJacket</ammoClass>
		<cookOffProjectile>Bullet_8mmGasser_FMJ</cookOffProjectile>
	</ThingDef>

	<ThingDef Class="CombatExtended.AmmoDef" ParentName="8mmGasserBase" MayRequire="cgf1.gunsgalore.greatwar.centralpowers">
		<defName>Ammo_8mmGasser_AP</defName>
		<label>8mm Gasser cartridge (AP)</label>
		<graphicData>
			<texPath>Things/Ammo/Pistol/AP</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>0.06</MarketValue>
		</statBases>
		<ammoClass>ArmorPiercing</ammoClass>
		<cookOffProjectile>Bullet_8mmGasser_AP</cookOffProjectile>
	</ThingDef>

	<ThingDef Class="CombatExtended.AmmoDef" ParentName="8mmGasserBase" MayRequire="cgf1.gunsgalore.greatwar.centralpowers">
		<defName>Ammo_8mmGasser_HP</defName>
		<label>8mm Gasser cartridge (HP)</label>
		<graphicData>
			<texPath>Things/Ammo/Pistol/HP</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>0.06</MarketValue>
		</statBases>
		<ammoClass>HollowPoint</ammoClass>
		<cookOffProjectile>Bullet_8mmGasser_HP</cookOffProjectile>
	</ThingDef>

	<!-- ================== Projectiles ================== -->

	<ThingDef Name="Base8mmGasserBullet" ParentName="BaseBulletCE" Abstract="true" MayRequire="cgf1.gunsgalore.greatwar.centralpowers">
		<graphicData>
			<texPath>Things/Projectile/Bullet_Small</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<projectile Class="CombatExtended.ProjectilePropertiesCE">
			<damageDef>Bullet</damageDef>
			<speed>50</speed>
			<dropsCasings>true</dropsCasings>
			<casingMoteDefname>Fleck_PistolAmmoCasings</casingMoteDefname>
			<casingFilthDefname>Filth_PistolAmmoCasings</casingFilthDefname>
		</projectile>
	</ThingDef>

	<ThingDef ParentName="Base8mmGasserBullet" MayRequire="cgf1.gunsgalore.greatwar.centralpowers">
		<defName>Bullet_8mmGasser_FMJ</defName>
		<label>8mm Gasser bullet (FMJ)</label>
		<projectile Class="CombatExtended.ProjectilePropertiesCE">
			<damageAmountBase>8</damageAmountBase>
			<armorPenetrationSharp>2.5</armorPenetrationSharp>
			<armorPenetrationBlunt>5</armorPenetrationBlunt>
		</projectile>
	</ThingDef>

	<ThingDef ParentName="Base8mmGasserBullet" MayRequire="cgf1.gunsgalore.greatwar.centralpowers">
		<defName>Bullet_8mmGasser_AP</defName>
		<label>8mm Gasser bullet (AP)</label>
		<projectile Class="CombatExtended.ProjectilePropertiesCE">
			<damageAmountBase>5</damageAmountBase>
			<armorPenetrationSharp>5</armorPenetrationSharp>
			<armorPenetrationBlunt>5</armorPenetrationBlunt>
		</projectile>
	</ThingDef>

	<ThingDef ParentName="Base8mmGasserBullet" MayRequire="cgf1.gunsgalore.greatwar.centralpowers">
		<defName>Bullet_8mmGasser_HP</defName>
		<label>8mm Gasser bullet (HP)</label>
		<projectile Class="CombatExtended.ProjectilePropertiesCE">
			<damageAmountBase>10</damageAmountBase>
			<armorPenetrationSharp>1.5</armorPenetrationSharp>
			<armorPenetrationBlunt>5</armorPenetrationBlunt>
		</projectile>
	</ThingDef>

	<!-- ==================== Recipes ========================== -->

	<RecipeDef ParentName="AmmoRecipeBase" MayRequire="cgf1.gunsgalore.greatwar.centralpowers">
		<defName>MakeAmmo_8mmGasser_FMJ</defName>
		<label>make 8mm Gasser cartridge x500</label>
		<description>Craft 500 8mm Gasser cartridges.</description>
		<jobString>Making 8mm Gasser cartridges.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Steel</li>
					</thingDefs>
				</filter>
				<count>14</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Steel</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Ammo_8mmGasser_FMJ>500</Ammo_8mmGasser_FMJ>
		</products>
		<workAmount>1400</workAmount>
	</RecipeDef>

	<RecipeDef ParentName="AmmoRecipeBase" MayRequire="cgf1.gunsgalore.greatwar.centralpowers">
