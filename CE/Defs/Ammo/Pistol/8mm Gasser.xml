	<RecipeDef ParentName="AmmoRecipeBase" MayRequire="cgf1.gunsgalore.greatwar.centralpowers">
		<defName>MakeAmmo_8mmGasser_HP</defName>
		<label>make 8mm Gasser (HP) cartridge x500</label>
		<description>Craft 500 8mm Gasser (HP) cartridges.</description>
