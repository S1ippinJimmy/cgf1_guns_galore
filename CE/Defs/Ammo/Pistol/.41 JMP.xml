		<graphicData>
			<texPath>Things/Ammo/Pistol/AP</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>0.09</MarketValue>
		</statBases>
		<ammoClass>ArmorPiercing</ammoClass>
		<cookOffProjectile>Bullet_41JMP_AP</cookOffProjectile>
	</ThingDef>

	<ThingDef Class="CombatExtended.AmmoDef" ParentName="41JMPBase" MayRequire="cgf1.gunsgalore.magnum.i">
