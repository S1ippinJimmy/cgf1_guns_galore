		<description>Craft 500 .41 JMP cartridges.</description>
		<jobString>Making .41 JMP cartridges.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Steel</li>
					</thingDefs>
				</filter>
				<count>22</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Steel</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Ammo_41JMP_FMJ>500</Ammo_41JMP_FMJ>
		</products>
		<workAmount>2200</workAmount>
	</RecipeDef>

