<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingCategoryDef>
		<defName>Ammo41JMP</defName>
		<label>.41 JMP</label>
		<parent>AmmoPistols</parent>
		<iconPath>UI/Icons/ThingCategories/CaliberPistol</iconPath>
	</ThingCategoryDef>

	<!-- ==================== AmmoSet ========================== -->

	<CombatExtended.AmmoSetDef MayRequire="cgf1.gunsgalore.magnum.i">
		<defName>AmmoSet_41JMP</defName>
		<label>.41 JMP</label>
		<ammoTypes>
			<Ammo_41JMP_FMJ>Bullet_41JMP_FMJ</Ammo_41JMP_FMJ>
			<Ammo_41JMP_AP>Bullet_41JMP_AP</Ammo_41JMP_AP>
			<Ammo_41JMP_HP>Bullet_41JMP_HP</Ammo_41JMP_HP>			
		</ammoTypes>
		<similarTo>AmmoSet_PistolMagnum</similarTo>
	</CombatExtended.AmmoSetDef>

	<!-- ==================== Ammo ========================== -->

	<ThingDef Class="CombatExtended.AmmoDef" Name="41JMPBase" ParentName="SmallAmmoBase" Abstract="True" MayRequire="cgf1.gunsgalore.magnum.i">
		<description>Mostly proprietary magnum cartridge intended to replicate the ballistics of the .41 Magnum in semi-automatic handguns.</description>
		<statBases>
			<Mass>0.022</Mass>
			<Bulk>0.02</Bulk>
		</statBases>
		<tradeTags>
			<li>CE_AutoEnableTrade</li>
			<li>CE_AutoEnableCrafting</li>
		</tradeTags>
		<thingCategories>
			<li>Ammo41JMP</li>
		</thingCategories>
	</ThingDef>

	<ThingDef Class="CombatExtended.AmmoDef" ParentName="41JMPBase" MayRequire="cgf1.gunsgalore.magnum.i">
		<defName>Ammo_41JMP_FMJ</defName>
		<label>.41 JMP cartridge (FMJ)</label>
		<graphicData>
			<texPath>Things/Ammo/Pistol/FMJ</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>0.09</MarketValue>
		</statBases>
		<ammoClass>FullMetalJacket</ammoClass>
		<cookOffProjectile>Bullet_41JMP_FMJ</cookOffProjectile>
	</ThingDef>

	<ThingDef Class="CombatExtended.AmmoDef" ParentName="41JMPBase" MayRequire="cgf1.gunsgalore.magnum.i">
