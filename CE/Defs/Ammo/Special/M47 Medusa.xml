<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- ==================== AmmoSet ========================== -->
	
	<!-- The full range of cartridges the M47 Medusa was claimed to able to shoot -->

	<CombatExtended.AmmoSetDef>
		<defName>AmmoSet_Special_M47Medusa</defName>
		<label>Multi-Caliber</label>
		<ammoTypes>
			<Ammo_9x19mmPara_FMJ>Bullet_9x19mmPara_FMJ</Ammo_9x19mmPara_FMJ>
			<Ammo_9x19mmPara_AP>Bullet_9x19mmPara_AP</Ammo_9x19mmPara_AP>
			<Ammo_9x19mmPara_HP>Bullet_9x19mmPara_HP</Ammo_9x19mmPara_HP>
			<Ammo_38ACP_FMJ>Bullet_38ACP_FMJ</Ammo_38ACP_FMJ>
			<Ammo_38ACP_AP>Bullet_38ACP_AP</Ammo_38ACP_AP>			
			<Ammo_38ACP_HP>Bullet_38ACP_HP</Ammo_38ACP_HP>
			<Ammo_38Super_FMJ>Bullet_38Super_FMJ</Ammo_38Super_FMJ>
			<Ammo_38Super_AP>Bullet_38Super_AP</Ammo_38Super_AP>
			<Ammo_38Super_HP>Bullet_38Super_HP</Ammo_38Super_HP>
			<Ammo_38Special_FMJ>Bullet_38Special_FMJ</Ammo_38Special_FMJ>
			<Ammo_38Special_AP>Bullet_38Special_AP</Ammo_38Special_AP>
			<Ammo_38Special_HP>Bullet_38Special_HP</Ammo_38Special_HP>
			<Ammo_38200SW_FMJ>Bullet_38200SW_FMJ</Ammo_38200SW_FMJ>
			<Ammo_38200SW_AP>Bullet_38200SW_AP</Ammo_38200SW_AP>
			<Ammo_38200SW_HP>Bullet_38200SW_HP</Ammo_38200SW_HP>	
			<Ammo_357Magnum_FMJ>Bullet_357Magnum_FMJ</Ammo_357Magnum_FMJ>
			<Ammo_357Magnum_AP>Bullet_357Magnum_AP</Ammo_357Magnum_AP>
			<Ammo_357Magnum_HP>Bullet_357Magnum_HP</Ammo_357Magnum_HP>
			<Ammo_380ACP_FMJ>Bullet_380ACP_FMJ</Ammo_380ACP_FMJ>
			<Ammo_380ACP_AP>Bullet_380ACP_AP</Ammo_380ACP_AP>
			<Ammo_380ACP_HP>Bullet_380ACP_HP</Ammo_380ACP_HP>
			<Ammo_32ACP_FMJ>Bullet_32ACP_FMJ</Ammo_32ACP_FMJ> <!-- Supposedly, .32 ACP can be shot out of it, albeit very ineffectually -->
			<Ammo_32ACP_AP>Bullet_32ACP_AP</Ammo_32ACP_AP>
			<Ammo_32ACP_HP>Bullet_32ACP_HP</Ammo_32ACP_HP>
			<Ammo_762x25mmTokarev_FMJ>Bullet_762x25mmTokarev_FMJ</Ammo_762x25mmTokarev_FMJ> <!-- ... as can 7.62x25 Tokarev, and by extension, 7.63 Mauser -->
			<Ammo_762x25mmTokarev_AP>Bullet_762x25mmTokarev_AP</Ammo_762x25mmTokarev_AP>
			<Ammo_762x25mmTokarev_HP>Bullet_762x25mmTokarev_HP</Ammo_762x25mmTokarev_HP>
			<Ammo_763x25mmMauser_FMJ>Bullet_763x25mmMauser_FMJ</Ammo_763x25mmMauser_FMJ>
			<Ammo_763x25mmMauser_AP>Bullet_763x25mmMauser_AP</Ammo_763x25mmMauser_AP>
			<Ammo_763x25mmMauser_HP>Bullet_763x25mmMauser_HP</Ammo_763x25mmMauser_HP>
			<!--
			<Ammo_9x18mmMakarov_FMJ>Bullet_9x18mmMakarov_FMJ</Ammo_9x18mmMakarov_FMJ>
			<Ammo_9x18mmMakarov_AP>Bullet_9x18mmMakarov_AP</Ammo_9x18mmMakarov_AP>
			<Ammo_9x18mmMakarov_HP>Bullet_9x18mmMakarov_HP</Ammo_9x18mmMakarov_HP> --> <!-- 9mm Makarov was an advertised caliber, but it cannot actually be fired out of an M47 Medusa. -->
		</ammoTypes>
		<similarTo>AmmoSet_Pistol</similarTo>
	</CombatExtended.AmmoSetDef>
</Defs>
