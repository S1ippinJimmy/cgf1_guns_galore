<?xml version="1.0" encoding="utf-8"?>
<Patch>
	
	<Operation Class="VEF.PatchOperationToggableSequence">
		<enabled>False</enabled>
		<label>[Core] Use preset gun spawns</label>
		<operations>
			<!-- Core -->
			<li Class="PatchOperationRemove">
				<xpath>/Defs/PawnKindDef[@Name="MercenaryGunnerBase" or defName="Villager" or defName="Town_Guard" or defName="Town_Trader" or defName="Town_Councilman" or defName="Scavenger" or defName="Pirate" or defName="AncientSoldier"]/weaponTags/li[text() = "Gun"]</xpath>
			</li>
			<li Class="PatchOperationRemove">
				<xpath>/Defs/PawnKindDef[defName="StrangerInBlack"]/weaponTags/li[text() = "Revolver"]</xpath>
			</li>
			<li Class="PatchOperationRemove">
				<xpath>/Defs/PawnKindDef[@Name="MercenarySniperBase"]/weaponTags/li[text() = "SniperRifle"]</xpath>
			</li>
			<li Class="PatchOperationRemove">
				<xpath>/Defs/PawnKindDef[defName="Mercenary_Heavy"]/weaponTags/li[text() = "GunHeavy"]</xpath>
			</li>
			<li Class="PatchOperationRemove">
				<xpath>/Defs/PawnKindDef[defName="Mercenary_Heavy"]/weaponTags/li[text() = "GunSingleUse"]</xpath>
			</li>
			<li Class="PatchOperationRemove">
				<xpath>/Defs/PawnKindDef[@Name="MercenaryEliteTierBase"]/weaponTags/li[text() = "IndustrialGunAdvanced"]</xpath>
			</li>
			<li Class="PatchOperationRemove">
				<xpath>/Defs/PawnKindDef[defName="Grenadier_Destructive"]/weaponTags/li[text() = "GrenadeDestructive"]</xpath>
			</li>
			<li Class="PatchOperationRemove">
				<xpath>/Defs/PawnKindDef[defName="Grenadier_EMP"]/weaponTags/li[text() = "GrenadeEMP"]</xpath>
			</li>
			<li Class="PatchOperationRemove">
				<xpath>/Defs/PawnKindDef[defName="Grenadier_Smoke"]/weaponTags/li[text() = "GrenadeSmoke"]</xpath>
			</li>
			
			<!--
			<li Class="PatchOperationReplace">
				<xpath>Defs/PawnKindDef[defName="Villager"]/weaponMoney</xpath>
				<value>
					<weaponMoney>65~510</weaponMoney>
				</value>
			</li>
			<li Class="PatchOperationReplace">
